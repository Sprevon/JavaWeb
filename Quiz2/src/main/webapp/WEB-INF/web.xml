    <init-param>
      <param-name>welcome</param-name>
      <param-value>welcome you</param-value>
    </init-param>
    <init-param>
      <param-name>name</param-name>
      <param-value>zhang san</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet2</servlet-name>
    <url-pattern>/quiz22</url-pattern>
  </servlet-mapping>

</web-app>
